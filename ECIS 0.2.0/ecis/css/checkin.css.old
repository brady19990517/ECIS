* {font-size:23px;font-family:Arial;}
 
#nav li {list-style:none; 
		width:100%; 
		margin-top:0;
		padding:4px 6px;
		}

#nav li a {text-decoration:none; color:#666;}

#nav li a:hover {color:#333;}

.aa {border-right:0px solid #AAC7E9; 
	background:#0F1E2C;
	cursor:hand;
	border-top:3px solid #F2F3F5;
    color:#F2F3F5;}

.dd {border-right:0px solid #AAC7E9; 
	background:#0F1E2C;
	cursor:hand;
	border-top:3px solid #0F1E2C;
    color:#F2F3F5;
    height:1000px;}

.bb {border:1px solid #AAC7E9;
	background:#FFFFDD;
	cursor:hand;
	border-top:3px solid #ff9900;}

.cc {border-top:0px solid #ff66ff; background:#black;}

